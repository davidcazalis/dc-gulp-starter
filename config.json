{
  "prod": "./build/",
  "libs": "./app/libs",
  "styles" : {
    "src" : "./app/styles/*.scss",
    "dest" : "./app/styles"
  },
  "templates" : {
    "src" : [
      "./app/views/*.jade",
      "!./app/views/_*.jade"
    ],
    "dest" : "./app/views"
  },
  "scripts" : {
    "src" : "./app/scripts/*.js"
  },
  "browserslist" : ["> 5%", "last 2 versions", "ie 9"]
}
